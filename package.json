{
  "name": "srcdev-nuxt-components",
  "type": "module",
  "version": "0.0.31",
  "main": "nuxt.config.ts",
  "scripts": {
    "clean": "rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output",
    "cleanall": "rm -rf node_modules && rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output && rm -rf .playground/node_modules && rm package-lock.json",
    "reinstall": "rm -rf node_modules && npm install",
    "cleaninstall": "npm run clean && npm run reinstall",
    "dev": "nuxi dev .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "postinstall": "nuxt prepare .playground",
    "release": "release-it",
    "storybook": "storybook dev --port 6006 --config-dir .storybook",
    "test": "vitest"
  },
  "files": [
    "assets/",
    "components/",
    "composables/",
    "plugins/",
    "types/"
  ],
  "devDependencies": {
    "@nuxt/eslint-config": "0.7.3",
    "@nuxt/icon": "1.10.2",
    "@oddbird/css-anchor-positioning": "0.4.0",
    "eslint": "9.17.0",
    "happy-dom": "15.11.7",
    "nuxt": "3.14.1592",
    "release-it": "17.10.0",
    "typescript": "5.7.2",
    "vue": "3.5.13"
  },
  "dependencies": {
    "@nuxtjs/storybook": "8.3.1",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/vue3": "8.4.7",
    "@vueuse/core": "12.0.0",
    "focus-trap-vue": "4.0.3",
    "http-proxy-middleware": "3.0.3",
    "modern-normalize": "3.0.1"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
